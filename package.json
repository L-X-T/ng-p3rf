{
  "name": "ng-p3rf",
  "version": "18.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "reset": "rm -rf node_modules && rm -f package-lock.json && npm i",
    "build:serve": "ng build && serve dist/ng-p3rf/browser -s",
    "serve": "serve dist/ng-p3rf/browser -s",
    "caddy": "caddy start",
    "caddy:stop": "caddy stop",
    "build:sme": "ng b --source-map && npm run sme",
    "sme": "npx source-map-explorer dist/ng-p3rf/browser/**/*.js --no-border-checks",
    "build:vbv": "ng b && npm run vbv",
    "vbv": "cd dist/ng-p3rf/browser && npx vite-bundle-visualizer && cd ../../..",
    "build:wba": "ng b --stats-json && npm run wba",
    "wba": "npx webpack-bundle-analyzer dist/ng-p3rf/stats.json",
    "igv": "npx @rx-angular/import-graph-visualizer --entry-points src/main.ts --ts-config tsconfig.app.json",
    "dpdm": "npx dpdm --no-warning --no-tree src/main.ts",
    "ng:update": "ng update @angular/cli @angular/core @angular/material @angular-eslint/schematics",
    "ssr": "node dist/ng-p3rf/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/paper-design": "^1.0.4",
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/platform-server": "^18.2.2",
    "@angular/router": "^18.2.0",
    "@angular/service-worker": "^18.2.2",
    "@angular/ssr": "^18.2.2",
    "anychart": "^8.12.1",
    "express": "^4.19.2",
    "moment": "^2.30.1",
    "ngx-quicklink": "^0.4.6",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.2",
    "@angular-eslint/builder": "^18.3.0",
    "@angular-eslint/eslint-plugin": "^18.3.0",
    "@angular-eslint/eslint-plugin-template": "^18.3.0",
    "@angular-eslint/schematics": "^18.3.0",
    "@angular-eslint/template-parser": "^18.3.0",
    "@angular/cli": "^18.2.2",
    "@angular/compiler-cli": "^18.2.2",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.16.3",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.3.3",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.5.4",
    "vite-bundle-visualizer": "^1.2.1",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
